<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <TextView
        android:id="@+id/te_textview"
        android:layout_width="200dp"
        android:layout_height="0dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="143dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="538dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/bt_logut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="로그아웃"
        app:layout_constraintEnd_toEndOf="@+id/te_textview"
        app:layout_constraintStart_toStartOf="@+id/te_textview"
        app:layout_constraintTop_toBottomOf="@+id/te_textview" />

    <Button
        android:id="@+id/bt_delect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="회원 탈퇴"
        app:layout_constraintEnd_toEndOf="@+id/bt_logut"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bt_logut"
        app:layout_constraintTop_toBottomOf="@+id/bt_logut" />

    <Button
        android:id="@+id/takepicture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Button"
        app:layout_constraintEnd_toEndOf="@+id/bt_delect"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bt_delect"
        app:layout_constraintTop_toBottomOf="@+id/bt_delect" />

    <ImageView
        android:id="@+id/takeimage"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginBottom="150dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/takepicture"
        app:layout_constraintTop_toBottomOf="@+id/te_textview" />

    <ImageView
        android:id="@+id/photoimage"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="46dp"
        android:layout_marginBottom="170dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/takeimage"
        app:layout_constraintTop_toBottomOf="@+id/te_textview" />

    <Button
        android:id="@+id/retry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="116dp"
        android:text="Button"
        app:layout_constraintEnd_toEndOf="@+id/takepicture"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/takepicture"
        app:layout_constraintTop_toBottomOf="@+id/takepicture" />




</androidx.constraintlayout.widget.ConstraintLayout>